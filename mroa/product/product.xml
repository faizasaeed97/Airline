<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="mroa_product_template_only_form_view_xpath">
        <field name="model">product.template</field>
        <field name="inherit_id" ref='product.product_template_only_form_view'/>
        <field name="arch" type="xml">
            <field name="default_code" position="attributes">
                <attribute name='invisible'>1</attribute>
            </field>
        </field>
    </record>
    <record model="ir.ui.view" id="mroa_product_form_xpaths">
        <field name="model">product.template</field>
        <field name="inherit_id" ref='product.product_template_form_view'/>
        <field name="arch" type="xml">
            <field name="type" position='before'>
                <field name="alias"/>
                <field name="is_assembly"/>
                <field name="serial_number" attrs="{'invisible':[('is_assembly','=',False)]}"/>
            </field>
            <label for="name" position="attributes">
                <attribute name='string'>Nomenclature</attribute>
            </label>
            <field name="type" position="after">
                <field name="required_ref" string="Internal Reference"/>
                <field name="part_number"/>
                <field name="alter_part_number"/>
                <field name="nsn_number"/>
                <field name="manufacturer_number"/>
                <field name="trade_code"/>
                <field name="manufacturer_name"/>
            </field>
            <field name="company_id" position="after">
                <field name="relation_code" invisible="1"/>
                <field name="serviceability"/>
            </field>
            <div name="options" position="inside">
                <div>
                    <field name="can_purchase_locally"/>
                    <label for="can_purchase_locally"/>
                </div>
                <div>
                    <field name="can_make_order"/>
                    <label for="can_make_order"/>
                </div>
            </div>
            <page name="inventory" position="before">
                <page string="Cross Reference" name="cross_reference">
                    <group>
                        <field name="cross_reference" nolabel="1">
                            <tree>
                                <field name="name" string="Nomenclature"/>
                                <field name="alias"/>
                                <field name="type"/>
                                <field name="categ_id"/>
                                <field name="required_ref"/>
                                <field name="relation_code"/>
                                <field name="serviceability"/>
                            </tree>
                        </field>
                    </group>
                </page>
            </page>
        </field>
    </record>
    <record model="ir.ui.view" id="mroa_product_template_tree_view">
        <field name="name">product.template.product.tree</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref='product.product_template_tree_view'/>
        <field name="arch" type="xml">
            <field name="default_code" position="replace">
                <field name="header_number_show"/>
            </field>
        </field>
    </record>
    <record id="mroa_product_template_kanban_view" model="ir.ui.view">
        <field name="name">Product.template.product.kanban</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref='product.product_template_kanban_view'/>
        <field name="arch" type="xml">
            <div class="oe_kanban_details" position="replace">
                <div class="oe_kanban_details">
                    <strong class="o_kanban_record_title">
                        <field name="name"/>
                    </strong>
                    <div t-if="record.tail_reference.value">
                        <field name="tail_reference"/>
                    </div>
                    <div t-if="record.product_variant_count.value &gt; 1" groups="product.group_product_variant">
                        <strong>
                            <t t-esc="record.product_variant_count.value"/> Variants
                        </strong>
                    </div>
                    <div name="tags"/>
                    <ul>
                        <li>Price: <field name="lst_price" widget="monetary"
                                          options="{'currency_field': 'currency_id', 'field_digits': True}"></field></li>
                    </ul>
                    <div name="tags"/>
                </div>
            </div>
        </field>
    </record>
    <record id="mroa_product_form_xpaths_two" model="ir.ui.view">
        <field name="name">product.template.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <field name="type" position="after">
                <field name="specific_type"/>
            </field>
            <label for="name" position="before">
                <h4><field name="header_number_show" readonly="1"/></h4>
            </label>
            <xpath expr="//sheet/notebook" position="inside">
                <page string="Serialized Inventory">
                    <group>
                        <group>
                            <label for="repair_condition"/>
                            <div class="o_row">
                                <field name="repair_condition"/>
                                <field name="repair_condition_value" invisible="1"/>
                            </div>
                        </group>
                        <group>
                            <field name="repair_Code" attrs="{'invisible':[('repair_condition_value','=','NoShow')]}"
                                   widget="many2many_tags"/>
                            <field name="select_mroa" widget="many2many_tags"/>
                        </group>
                    </group>
                </page>
            </xpath>
            <xpath expr="//sheet/notebook" position="inside">
                <page string="Tools-Testbenches-GSE"
                      attrs="{'invisible': [('specific_type', '!=', 'Tools, Testbenches, GSE')]}">
                    <field name="tools_testbenches" nolabel="1">
                        <tree>
                            <field name="TTG" width="7"/>
                            <field name="Purpose_and_Description" width="15"/>
                            <field name="Dimensions" width="10"/>
                            <field name="measuring_value" width="15"/>
                            <field name="aircraft_type" width="10" widget="many2many_tags"/>
                            <field name="test_conducted" width="10"/>
                        </tree>
                        <form string="Tools Test Benches GSE">
                            <group>
                                <group>
                                    <label for="TTG"/>
                                    <div class="o_row">
                                        <field name="TTG"/>
                                    </div>
                                    <label for="Purpose_and_Description" style="width:160px"/>
                                    <div class="o_row">
                                        <field name="Purpose_and_Description"/>
                                    </div>
                                    <label for="Dimensions"/>
                                    <div class="o_row">
                                        <field name="Dimensions"/>
                                    </div>
                                    <field name="measuring_value"
                                           attrs="{'invisible':[('what_ttg','=','General Tools')]}"/>
                                </group>
                                <group>
                                    <label for="measurement_ranges_with_tolerances" style="width:240px"
                                           attrs="{'invisible':[('what_ttg','=','General Tools')]}"/>
                                    <div class="o_row">
                                        <field name="measurement_ranges_with_tolerances"
                                               attrs="{'invisible':[('what_ttg','=','General Tools')]}"/>
                                    </div>
                                    <field name="used_for_assemblies" widget="many2many_tags"
                                           attrs="{'invisible':[('what_ttg','in',['Special Tools','Ground Support Equipment, GSE'])]}"/>
                                    <field name="aircraft_type" widget="many2many_tags"
                                           attrs="{'invisible':[('what_ttg','in',['General Tools','Testbenches','Test measuring and digonastic Equioment, TMDEs'])]}"/>
                                    <field name="test_conducted"
                                           attrs="{'invisible':[('what_ttg','in',['General Tools'])]}"/>
                                    <field name="what_ttg" invisible="1"/>
                                </group>
                            </group>
                            <group string="Specifications">
                                <field name="tools_specification" nolabel="1">
                                    <tree editable="bottom">
                                        <field name="name"/>
                                        <field name="description"/>
                                    </tree>
                                </field>
                            </group>
                        </form>
                    </field>
                </page>
            </xpath>
            <xpath expr="//sheet/notebook" position="inside">
                <page string="Consumables" attrs="{'invisible': [('type', '!=', 'consu')]}">
                    <group>
                        <group>
                            <field name="type_prod"/>
                        </group>
                        <group>
                            <field name="local_manufacturer_multiple_selection" widget="many2many_tags"/>
                            <field name="local_vendors_multiple_selection" widget="many2many_tags"/>
                        </group>
                    </group>
                    <group string="Specification">
                        <field name="specification_prod" nolabel="1">
                            <tree editable="bottom">
                                <field name="name"/>
                                <field name="unit"/>
                                <field name="con_prod_tmp_id" invisible="1"/>
                            </tree>
                        </field>
                    </group>
                </page>
                <page string="Standard Parts" attrs="{'invisible': [('specific_type', '!=', 'Standard Parts')]}">
                    <group>
                        <group>
                            <field name="sparts_type_prod"/>
                        </group>
                        <group>
                            <field name="spart_manufacturer_multiple_selection" widget="many2many_tags"/>
                        </group>
                    </group>
                    <group string="Specification">
                        <field name="spart_specification_prod" nolabel="1">
                            <tree editable="bottom">
                                <field name="name"/>
                                <field name="unit"/>
                                <field name="spart_prod_tmp_id" invisible="1"/>
                            </tree>
                        </field>
                    </group>
                </page>
                <page string="Expendables" attrs="{'invisible': [('specific_type', '!=', 'Expendables')]}">
                    <group>
                        <group>
                            <field name="expand_type_prod"/>
                        </group>
                        <group>
                            <field name="expand_manufacturer_multiple_selection" widget="many2many_tags"/>
                        </group>
                    </group>
                    <group string="Specification">
                        <field name="expand_specification_prod" nolabel="1">
                            <tree editable="bottom">
                                <field name="name"/>
                                <field name="unit"/>
                                <field name="expand_prod_tmp_id" invisible="1"/>
                            </tree>
                        </field>
                    </group>
                </page>
                <page string="Tail Nowise Aircraft Inventory">
                    <group>
                        <group>
                            <label for="aircraft_type" style="width:200px"/>
                            <div class="o_row">
                                <field name="aircraft_type"/>
                            </div>
                            <label for="tail_reference" style="width:200px"/>
                            <div class="o_row">
                                <field name="tail_reference"/>
                            </div>
                            <label for="tbo_expendables_serial_number" style="width:200px"/>
                            <div class="o_row">
                                <field name="tbo_expendables_serial_number"/>
                            </div>
                            <label for="manufacture_date" style="width:200px"/>
                            <div class="o_row">
                                <field name="manufacture_date"/>
                            </div>
                            <label for="date_induction_purchase" style="width:200px"/>
                            <div class="o_row">
                                <field name="date_induction_purchase"/>
                            </div>
                            <field name="tail_internal_reference" invisible="1"/>
                        </group>
                        <group>
                            <field name="tail_number" domain="[('model_id','=',aircraft_type)]"/>
                            <field name="installation_date"/>
                            <field name="removal_date"/>
                            <field name="removal_reason"/>
                            <field name="last_visit"/>
                        </group>
                    </group>
                    <group>
                        <group>
                            <label for="date_last_visit_workshop_depot" style="width:250px"/>
                            <div class="o_row">
                                <field name="date_last_visit_workshop_depot"/>
                            </div>
                        </group>
                        <group>
                            <label for="fault_details_authority" style="width:250px"/>
                            <div class="o_row">
                                <field name="fault_details_authority"/>
                            </div>
                        </group>
                    </group>
                </page>
                <page name="forecasting_page" string="Forecasting &amp; LSP">
                    <field name="forecasting_ids">
                        <tree>
                            <field name="lsp_designer" width="15"/>
                            <field name="project_quantity" width="20"/>
                            <field name="edp_quantity" width="20"/>
                            <field name="un_quantity" width="20"/>
                        </tree>
                        <form>
                            <group>
                                <group>
                                    <field name="lsp_designer"/>
                                </group>
                                <group>
                                    <label for="project_quantity" style="width: 190px"/>
                                    <div class="o_row">
                                        <field name="project_quantity"/>
                                    </div>
                                    <label for="edp_quantity"/>
                                    <div class="o_row">
                                        <field name="edp_quantity"/>
                                    </div>
                                    <label for="un_quantity"/>
                                    <div class="o_row">
                                        <field name="un_quantity"/>
                                    </div>
                                </group>
                            </group>
                            <notebook>
                                <page string="Forecasted Quantity">
                                    <field name="forecasted_quantity">
                                        <tree>
                                            <field name="f_quantity_sch"/>
                                            <field name="f_quantity_unf"/>
                                            <field name="f_quantity_tot"/>
                                        </tree>
                                        <form>
                                            <group>
                                                <label for="f_quantity_sch" style="width:320px"/>
                                                <div class="o_row">
                                                    <field name="f_quantity_sch"/>
                                                </div>
                                                <field name="f_quantity_unf"/>
                                                <field name="f_quantity_tot"/>
                                            </group>
                                        </form>
                                    </field>
                                </page>
                                <page string="LSP Quantity">
                                    <field name="lsp_quantity">
                                        <tree>
                                            <field name="lsp_quantity_sch"/>
                                            <field name="lsp_quantity_unf"/>
                                            <field name="lsp_quantity_tot"/>
                                        </tree>
                                        <form>
                                            <group>
                                                <label for="lsp_quantity_sch" style="width:300px"/>
                                                <div class="o_row">
                                                    <field name="lsp_quantity_sch"/>
                                                </div>
                                                <field name="lsp_quantity_unf"/>
                                                <field name="lsp_quantity_tot"/>
                                            </group>
                                        </form>
                                    </field>
                                </page>
                            </notebook>
                        </form>
                    </field>
                </page>
            </xpath>
        </field>
    </record>
    <record id="mroa_product_form_xpaths_three" model="ir.ui.view">
