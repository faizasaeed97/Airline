<?xml version="1.0" ?>
<odoo>
<data noupdate="0">
    <record id="module_fleet_category" model="ir.module.category">
        <field name="name">Fleet vehicle</field>
        <field name="sequence">17</field>
    </record>
    <record id="fleet_group_user_own" model="res.groups">
        <field name="name">User Own Document</field>
        <field name="category_id" ref="module_fleet_category"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

        <record id="fleet_group_user_alls" model="res.groups">
        <field name="name">User All Document</field>
        <field name="category_id" ref="module_fleet_category"/>
        <field name="implied_ids" eval="[(4, ref('fleet_group_user_own'))]"/>
    </record>

    <record id="fleet_group_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="implied_ids" eval="[(4, ref('fleet_group_user_alls'))]"/>
        <field name="category_id" ref="module_fleet_category"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>


    <record id="base.default_user" model="res.users">
        <field name="groups_id" eval="[(4,ref('fleet_group_manager'))]"/>
    </record>


    <record id="fleet_rule_contract_visibility_user_own" model="ir.rule">
        <field name="name">User can only see his/her vehicles</field>
        <field name="model_id" ref="model_vehicle"/>
        <field name="groups" eval="[(4, ref('fleet_group_user_own'))]"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_create" eval="True"/>
        <field name="perm_unlink" eval="False"/>
        <field name="domain_force">['|', ('create_uid', '=', user.id), ('create_uid', '=', False)]</field>
    </record>


            <record id="fleet_rule_contract_visibility_user_all" model="ir.rule">
        <field name="name">User can see modify all vehicles</field>
        <field name="model_id" ref="model_vehicle"/>
        <field name="groups" eval="[(4, ref('fleet_group_user_alls'))]"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_create" eval="True"/>
        <field name="perm_unlink" eval="False"/>
        <field name="domain_force">[(1,'=',1)]</field>
    </record>

